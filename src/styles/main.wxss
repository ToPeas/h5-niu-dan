@keyframes egg {
  0% {
    transform: translate3d(0, -60px, 0);
    opacity: 0.3;
  }
  20% {
    transform: translate3d(0, -40px, 0);
    opacity: 0.6;
  }
  40% {
    transform: translate3d(0, -20px, 0);
    opacity: 0.9;
  }
  60% {
    transform: translate3d(0, 0, 0);
    opacity: 1;
  }
  65% {
    transform: translate3d(0, 10px, 0);
    transform: scaleX(1.2);
  }
  70% {
    transform: translate3d(0, 25px, 0);
    transform: scaleX(1.4);
  }
  75% {
    transform: translate3d(0, 15px, 0);
    transform: scaleX(1.3);
  }
  80% {
    transform: translate3d(0, 5px, 0);
    transform: scaleX(1.2);
  }
  85% {
    transform: translate3d(0, 0, 0);
    transform: scaleX(1);
  }
  100% {
    transform: translate3d(0, 0, 0);
  }
}
@keyframes mask-egg-up {
  0% {
    transform: translate3d(0, -12px, 0);
  }
  100% {
    transform: translate3d(0, -262px, 0);
  }
}
@keyframes mask-card-up {
  0% {
    transform: translate3d(0, -42, 0);
  }
  20% {
    transform: translate3d(0, -42, 0);
    opacity: 1;
  }
  100% {
    transform: translate3d(0, -152px, 0);
    opacity: 1;
  }
}
@keyframes scale {
  0% {
    transform: scale(1);
    /*开始为原始大小*/
  }
  25% {
    transform: scale(0.4);
    /*放大1.1倍*/
  }
  50% {
    transform: scale(1);
  }
  75% {
    transform: scale(0.4);
  }
}
@keyframes play-png {
  0% {
    background-position: 0px 0px;
  }
  5% {
    background-position: -564px 0px;
  }
  10% {
    background-position: -1128px 0px;
  }
  15% {
    background-position: -1692px 0px;
  }
  20% {
    background-position: 0px -400px;
  }
  25% {
    background-position: -564px -400px;
  }
  30% {
    background-position: -1128px -400px;
  }
  35% {
    background-position: -1692px -400px;
  }
  40% {
    background-position: 0px -800px;
  }
  45% {
    background-position: -564px -800px;
  }
  50% {
    background-position: -1128px -800px;
  }
  55% {
    background-position: -1692px -800px;
  }
  60% {
    background-position: 0px -1200px;
  }
  65% {
    background-position: -564px -1200px;
  }
  70% {
    background-position: -1128px -1200px;
  }
  75% {
    background-position: -1692px -1200px;
  }
  80% {
    background-position: 0 -1600px;
  }
  85% {
    background-position: -564px -1600px;
  }
  90% {
    background-position: -1128px -1600px;
  }
  95% {
    background-position: -1692px -1600px;
  }
  100% {
    background-position: 0 0;
  }
}
